{
  "name": "Mole - Columbia People Directory Scrapper",
  "version": "1.0",
  "description": "to be used in Quaffles",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "css": ["styles.css"],
      "js": ["content.js"],
      "matches": ["https://directory.columbia.edu/people/search*"]
    }
  ],
  "permissions": ["tabs"],
  "manifest_version": 2
}
